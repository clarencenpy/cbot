<div class="special card studentCard" data-studentid=<%= student._id %>>
    <div class="dimmable image">
        <div class="ui inverted dimmer">
            <div class="content">
                <div class="center">
                    <div class="ui blue button">Button!</div>
                </div>
            </div>
        </div>
        <img src="<%= student.displayPhoto %>">
    </div>
    <div class="content">
        <div class="header"><%= student.firstName %></div>
    </div>
    <div class="extra content">
        <div class="ui indicating small progress">
            <div class="bar"></div>
            <div class="label"></div>
        </div>
    </div>
</div>
