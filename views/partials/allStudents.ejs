<div id="students" class="ui padded segment">
    <h2 class="ui header">
        <i class="users icon"></i>
        <div class="content">Class Roster</div>
    </h2>
    <hr>
    <br>
    <div id="studentList" class="ui four doubling cards">
        <% students.forEach(student => { %>
        <%- include('studentCard', {student}) %>
        <% }) %>
    </div>
    <% if (students.length === 0) { %>
    <div id="studentEmptyNotice" class="empty-notice">
        <h4 class="ui icon header">
            <i class="help icon"></i>
            <div class="content">
                No students yet
            </div>
        </h4>
    </div>
    <% } %>
</div>

